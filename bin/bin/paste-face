#!/bin/sh

# data hack stolen from x0
if selected=$(sed '0,/^__DATA__$/d' "$0" | dmenu); then
    selected=$(echo "$selected" | sed 's/ .*//')
    printf '%s' "$selected" | xclip -selection clipboard -i
    printf '%s' "$selected" | xclip -selection primary -i

    # paste it
    xdotool key shift+Insert
fi

exit

