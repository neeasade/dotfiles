#!/usr/bin/env bb
;; -*- mode: clojure -*-
;; generate telegram themes
;; :label "#ffffffAA" (value with optional opacity)
;; :label :label (pointer to another value)
;; :label :label#opacity (some other value with opacity applied)
;; :_label <color> (value that will not be exported, for internal resolution)

(require '[clojure.string :as string])

(defn render-theme [theme]
  (->> theme
       (map
        (fn resolve [[label value]]
          (if (keyword? value)
            (let [[value-ref opacity] (string/split (str value) #"%")
                  value-ref-label? (string/starts-with? value-ref ":")]
              [label
               (if (nil? opacity)
                 (if value-ref-label?
                   (let [deref (read-string value-ref)]
                     (second (resolve [deref (deref theme)]))) value-ref)

                 (if value-ref-label?
                   (let [deref (read-string value-ref)]
                     (format "%s%%%s" (second (resolve [deref (deref theme)])) opacity))
                   (format "%s%%%s" value-ref opacity)))])
            [label value])))
       (mapcat
        (fn [[label value]]
          (let [[value & opacities] (string/split (str value) #"%")
                opacity (last opacities)]
            {label
             (if (nil? opacity)
               value
               (format "%s%x"
                       value
                       (->> (Integer/parseInt opacity)
                            (* 255)
                            (* 0.01)
                            (int))))})))
       (remove (fn [[k _v]] (string/starts-with? (str k) ":_")))
       (into {})))

