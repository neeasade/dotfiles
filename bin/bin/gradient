#!/usr/bin/env elisp
;; note: not inclusive -- just the steps inbetween the colors

(ns/let-script-args
 (step start end)
 (ns/make-lines
  (mapcar (fn (eval `(color-rgb-to-hex ,@<> 2)))
	  (color-gradient
	   (color-name-to-rgb (format "#%s" start))
	   (color-name-to-rgb (format "#%s" end))
	   (string-to-number step)))))
