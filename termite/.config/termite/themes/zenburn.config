[options]
scroll_on_output = false
scroll_on_keystroke = true
audible_bell = false
mouse_autohide = false
allow_bold = false
dynamic_title = true
urgent_on_bell = true
clickable_url = true
font = Terminus 8
scrollback_lines = 10000
search_wrap = true
#icon_name = terminal
#geometry = 640x480

# "system", "on" or "off"
cursor_blink = off

# "block", "underline" or "ibeam"
cursor_shape = block

# $BROWSER is used by default

# set size hints for the window
#size_hints = false

# emit escape sequences for other keys modified by Control
# modify_other_keys = false

[colors]

# special
foreground      = #dcdccc
foreground_bold = #dcdccc
cursor          = #dcdccc
background      = #3f3f3f

# black
color0  = #3f3f3f
color8  = #666666

# red
color1  = #9e1828
color9  = #cf6171

# green
color2  = #aece92
color10 = #c5f779

# yellow
color3  = #968a38
color11 = #fff796

# blue
color4  = #616191
color12 = #4186be

# magenta
color5  = #963c59
color13 = #cf9ebe

# cyan
color6  = #418179
color14 = #71bebe

# white
color7  = #bebebe
color15 = #ffffff

# 20% background transparency (requires a compositor)
#background = rgba(63, 63, 63, 0.8)

# if unset, will reverse foreground and background
#highlight = #2f2f2f

[hints]
#font = Dejavu Sans Mono 12
#foreground = #dcdccc
#background = #3f3f3f
#active_foreground = #e68080
#active_background = #3f3f3f
#padding = 2
#border = #3f3f3f
#border_width = 0.5
#roundness = 2.0

# vim: ft=dosini cms=#%s
